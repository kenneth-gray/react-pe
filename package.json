{
  "name": "react-pe",
  "version": "0.1.0",
  "description": "Progressive enhancement for React websites",
  "main": "dist/progressive-enhancement.js",
  "scripts": {
    "lint": "eslint . --ext js,ts,tsx && tsc -p . --noEmit",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "test": "jest",
    "dev": "concurrently --kill-others \"tsc -p tsconfig.build.json -w\" \"next dev example\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kenneth-gray/react-pe.git"
  },
  "files": [
    "dist"
  ],
  "author": "Kenneth Gray",
  "bugs": {
    "url": "https://github.com/kenneth-gray/react-pe/issues"
  },
  "homepage": "https://github.com/kenneth-gray/react-pe#readme",
  "devDependencies": {
    "@reach/accordion": "^0.16.1",
    "@types/jest": "^27.0.1",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.0",
    "next": "^11.1.2",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "react": ">=16.8.6 <18"
  },
  "keywords": [
    "react",
    "progressive enhancement"
  ]
}
